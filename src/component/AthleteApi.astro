---
import type { Athlete } from "../models";
const API = 'http://localhost:3000/api/v1/athletes?team=Alliance';
const response = await fetch(API);
const data = await response.json() as Athlete[];
---

<div class='grid grid-5 not-content'>
	{data.map((item) => (
    <article>
      <h4>{item.name}</h4>
      <h4>{item.lastName}</h4>
      <h4>{item.team}</h4>
      <p>{item.email}</p>
    </article>
  ))}
  
</div>